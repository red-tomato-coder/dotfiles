{
  "debug": {
    "disable_auto_focus": false,
    "enable_logging": false
  },
  "theme": {
    "colors": {
      "border": "#333333",
      "item_bg": "#3b3e47",
      "item_bg_hover": "#3b3e47",
      "item_bg_selected": "#3b3e47",
      "item_description": "#e0e0e0",
      "item_name": "#b8b8b8",
      "item_path": "#808080",
      "search_bg": "#1f1f1f",
      "search_bg_focused": "#282828",
      "search_caret": "#808080",
      "search_text": "#ffffff",
      "window_bg": "#2b2e37"
    },
    "corners": {
      "list_item": 8,
      "search": 8,
      "window": 12
    },
    "spacing": {
      "item_margin": 6,
      "item_padding": 4,
      "search_margin": 12,
      "search_padding": 12
    },
    "typography": {
      "item_description_size": 12,
      "item_name_size": 14,
      "item_path_font_family": "monospace",
      "item_path_size": 12,
      "search_font_size": 16
    }
  },
  "window": {
    "anchor": "center",
    "border_width": 2,
    "custom_navigate_keys": {
      "delete_word": "h",
      "down": "j",
      "up": "k"
    },
    "height": 600,
    "margin_bottom": 0,
    "margin_left": 0,
    "margin_right": 0,
    "margin_top": 0,
    "max_entries": 50,
    "show_border": true,
    "show_descriptions": false,
    "show_icons": true,
    "show_paths": false,
    "show_search": true,
    "use_gtk_colors": false,
    "width": 600
  },
 }
